{
	"schemaVersion": 1,
	"id": "mob-mod",
	"version": "${version}",
	"name": "Mob Mod",
	"description": "This is an example description! Tell everyone what your mod is about!",
	"authors": [
		"Me!"
	],
	"contact": {
		"homepage": "https://fabricmc.net/",
		"sources": "https://github.com/FabricMC/fabric-example-mod"
	},
	"license": "CC0-1.0",
	"icon": "assets/mob-mod/icon.png",
	"environment": "*",
	"entrypoints": {
		"main": [
			"com.example.name.modid.MobMod"
		],
		"fabric-datagen": [
			"com.example.name.modid.MobModDataGenerator"
		],
		"client": [
			"com.example.name.modid.MobModClient"
		]
	},
	"mixins": [
		"mob-mod.mixins.json"
	],
	"depends": {
		"fabricloader": ">=0.15.7",
		"minecraft": "~1.20.4",
		"java": ">=17",
		"fabric-api": "*"
	},
	"suggests": {
		"another-mod": "*"
	}
}